          <material>
            <mattext texttype="text/html">&lt;div&gt;This course uses a weighted grading system, what weights are used for this class?&lt;/div&gt;</mattext>
          </material>
          <response_lid ident="response1" rcardinality="Single">
            <render_choice>
              <response_label ident="63272">
                <material>
